datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Ticket {
  id              Int      @id @default(autoincrement())
  title           String
  description     String
  requesttypes_id Int
  urgencyText     String
  userRequest     String?
  glpiTicketId    Int
  phone           String
  createdAt       DateTime @default(now())
}
